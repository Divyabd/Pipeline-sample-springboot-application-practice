pipeline{
    agent {
    docker{
     image 'maven:3-alpine'
        args '-v/.m2:/root/.m2'
    }
  }
 stages{
     stage('maven clean '){
     steps{
         echo "maven clean"
         sh 'mvn clean'

     }
     }
     stage('maven  package'){
     steps{
         echo "maven package"
         sh 'mvn package'

     }
     }
 }
}